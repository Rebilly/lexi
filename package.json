{
    "name": "readability-reporter",
    "version": "0.1",
    "description": "Reports the readability scores for a pull request",
    "author": "Rebilly Inc",
    "repository": "https://github.com/Rebilly/readability-reporter",
    "main": "index.js",
    "scripts": {
        "build": "rollup -c",
        "test": "jest",
        "format": "prettier --write src/*.js src/**/*.js ./*.js",
        "run:local:report": "babel-node ./src/cli/report",
        "run:local:debugfile": "babel-node ./src/cli/debug-file.js",
        "lint": "eslint --ext .js ."
    },
    "dependencies": {
        "@actions/core": "^1.9.1",
        "@actions/exec": "^1.1.0",
        "@actions/github": "^5.0.0",
        "glob": "^7.1.7",
        "remark": "^13.0.0",
        "strip-markdown": "^4.0.0",
        "text-readability": "^1.0.5",
        "unist-util-visit": "2.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/node": "^7.14.7",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.0.6",
        "commander": "^9.4.0",
        "eslint": "^7.29.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "3.4.0",
        "jest": "^27.0.6",
        "prettier": "^2.3.2",
        "rollup": "^2.52.4"
    },
    "jest": {
        "testMatch": [
            "<rootDir>/src/**/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "bracketSpacing": false
    },
    "resolutions": {
        "text-readability/syllable/trim": "^0.0.3"
    }
}
